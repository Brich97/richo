{
    "builds": [
      {
        "src": "vite.config.ts",
        "use": "@vercel/static-build",
        "config": {
        "distDir": "dist"
      }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/index.html"
      }
    ],
    "rewrites": [
    { "source": "/app/(.*)", "destination": "/$1" }
  ]
  }
  